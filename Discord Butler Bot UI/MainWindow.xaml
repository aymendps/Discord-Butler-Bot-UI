<Window x:Class="Discord_Butler_Bot_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Discord_Butler_Bot_UI"
        mc:Ignorable="d"
        Title="Discord Butler Bot UI" Height="450" Width="800" Background="{StaticResource BgColor}" FontFamily="{StaticResource PrimaryFontFamily}">
    <Grid>
        <Button Name="StartBot" Style="{StaticResource MainButton}" Content="Start Bot" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,25,0,0" Height="50" Width="150" Click="Start_Bot_Click"/>
    </Grid>
</Window>
